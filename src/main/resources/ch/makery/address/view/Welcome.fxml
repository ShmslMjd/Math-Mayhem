<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.WelcomeController">
   <children>
      <Text layoutX="244.0" layoutY="299.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Math Mayhem">
         <font>
            <Font name="Garamond Bold" size="50.0" />
         </font>
      </Text>
      <Text layoutX="280.0" layoutY="354.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Practice make perfect!">
         <font>
            <Font name="Garamond" size="28.0" />
         </font>
      </Text>
      <Button fx:id="startBtn" layoutX="336.0" layoutY="445.0" mnemonicParsing="false" onAction="#getStarted" prefHeight="49.0" prefWidth="128.0" text="Start Game">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="abtBtn" layoutX="336.0" layoutY="511.0" mnemonicParsing="false" onAction="#showAbout" prefHeight="49.0" prefWidth="128.0" text="About">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="quitBtn" layoutX="336.0" layoutY="579.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="49.0" prefWidth="128.0" text="Quit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="325.0" layoutY="95.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/math.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
